library("png") # for reading in PNGs
require(dplyr)
library("grid")
library("ggplotify")
require(gridExtra)
# example image
# https://bookdown.org/ndphillips/YaRrr/arranging-plots-with-parmfrow-and-layout.html
imglist = list.files("C:/Users/Maia Kapur/Dropbox/UW/PhD Writing/CH0_proposal/",
pattern = "*PNG") %>% lapply(., readPNG)
png("./figure2.png", width = 10, height = 8, units = 'in', res = 520)
par(mar=rep(0,4)) # no margins
layout.matrix <- matrix(c(1,1,1,1,2,2,3,3), byrow = T, nrow = 2, ncol = 4)
layout(mat = layout.matrix,
heights = c(1, 2), # Heights of the two rows
widths = c(2, 2,2,2)) # Widths of the two columns
# layout.show(3)
plot(NA,xlim=0:1,ylim=0:1,xaxt="n",yaxt="n",bty="n")
rasterImage(imglist[[2]],0,0,1,1)
plot(NA,xlim=0:1,ylim=0:1,xaxt="n",yaxt="n",bty="n")
rasterImage(imglist[[1]],0,0,1,1)
plot(NA,xlim=0:1,ylim=0:1,xaxt="n",yaxt="n",bty="n")
rasterImage(imglist[[3]],0,0,1,1)
dev.off()
